---
import Layout from "../layouts/Layout.astro";
import Bookmarks from "../components/Bookmarks";
import bookmarks from "../data/bookmarks.json";
---

<Layout title="My frequently used apps">
  <main
    class="dark:bg-slate-900 bg-slate-50 h-screen text-slate-50 mx-auto max-w-6xl p-10"
  >
    <Bookmarks bookmarks={bookmarks} client:load/>
  </main>
</Layout>
